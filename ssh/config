Host router
  HostName 192.168.1.1
  User pedram
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
  StrictHostKeyChecking accept-new
  UpdateHostKeys yes

Host devbox
  HostName ped-pc-win
  User p
  AddKeysToAgent yes
  Port 10022

Host *
  IdentityFile ~/.ssh/pedram
  StrictHostKeyChecking accept-new
  UpdateHostKeys yes
  ConnectionAttempts 5
