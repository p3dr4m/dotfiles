Host router
  HostName 192.168.1.1
  User pedram
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
  StrictHostKeyChecking accept-new
  UpdateHostKeys yes

Host *
  AddKeysToAgent yes
  IdentityFile ~/.ssh/pedram
  IdentityAgent ~/.ssh/agent.sock
  StrictHostKeyChecking accept-new
  UpdateHostKeys yes
  ConnectionAttempts 5
